{
  "name": "nodejs-express-template",
  "version": "1.0.0",
  "description": "My personal template to use to accelerate project setup in Node.js/Express",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup",
    "ci": "npm ci && npm run format && npm run test:unit && npm run lint",
    "test": "jest --config jest.config.test.ts",
    "lint": "eslint ./eslint.config.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typeorm": "node --loader ts-node/esm ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d dist/database/data-source.js src/database/migrations/Init",
    "migration:run": "npm run typeorm -- migration:run -d dist/database/data-source.js",
    "migration:revert": "npm run typeorm -- migration:revert -d dist/database/data-source.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YoUnGi102/NodeJS-Express-Template.git"
  },
  "keywords": [
    "node.js",
    "typescript",
    "express",
    "template"
  ],
  "author": "YoUnGi102 (Tomáš Greš)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/YoUnGi102/NodeJS-Express-Template/issues"
  },
  "homepage": "https://github.com/YoUnGi102/NodeJS-Express-Template#readme",
  "dependencies": {
    "@goodrequest/joi-type-extract": "^0.0.4",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "eslint": "^9.27.0",
    "express": "^5.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.24",
    "typescript-eslint": "^8.32.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "depcheck": "^1.4.7",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.5.3",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-transform-paths": "^3.5.5"
  }
}
